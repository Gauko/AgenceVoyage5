<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

   <!-- Servlets -->
 <servlet>
  <servlet-name>GreetingServiceServlet</servlet-name>
  <servlet-class>m2tiil.agence.voyage.server.GreetingServiceImpl</servlet-class>
 </servlet>
 
 <servlet-mapping>
   <servlet-name>GreetingServiceServlet</servlet-name>
   <url-pattern>/agencevoyage5/greet</url-pattern>
 </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>AgenceVoyage5.html</welcome-file>
  </welcome-file-list>

</web-app>
