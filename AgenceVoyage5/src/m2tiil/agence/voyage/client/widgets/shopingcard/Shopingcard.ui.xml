<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	<g:FlowPanel styleName="{style.panel}">
		<p1:CellTable ui:field="cellTable" width="449px" pageSize="1"/>
		<g:VerticalPanel width="447px" height="35px" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
			<g:Button text="Buy" ui:field="buttonBuy" width="65px"/>
		</g:VerticalPanel>
	</g:FlowPanel>
</ui:UiBinder>
